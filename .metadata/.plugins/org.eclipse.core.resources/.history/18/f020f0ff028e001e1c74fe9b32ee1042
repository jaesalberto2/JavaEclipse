/*
 * Ejercicio 9; Definicion de numeros random
 */


package metodos;

public class Random1 {

	public static void main(String[] args) {
		// Declaracion de variables
		int nrandom;
		double random;
		random=Math.random();
		nrandom=(int)(random*100);
		System.out.println(nrandom);
		System.out.println(random*10);
	
	}

}
